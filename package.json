{
  "name": "react-app",
  "version": "1.0.0",
  "description": "A demo application built on react+redux",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com> (http://www.mauriziocescon.com)",
  "homepage": "http://www.mauriziocescon.com",
  "private": false,
  "engines": {
    "node": "=8.11.3",
    "npm": "=5.6.0"
  },
  "scripts": {
    "postinstall": "node ./mock-server/db/db-creator.js | npm run build",
    "start": "npm run json-server",
    "json-server": "node ./mock-server/server.js",
    "json-server-watch": "nodemon --ext js --watch ./mock-server ./mock-server/server.js",
    "serve": "webpack-dev-server --d --progress --profile --colors --env.name=dev | npm run json-server",
    "build": "webpack --progress --profile --colors --env.name=prod",
    "test": "jest src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/react-app"
  },
  "keywords": [
    "bootstrap",
    "cescon",
    "es2017",
    "json-server",
    "maurizio",
    "mock-server",
    "react",
    "react-intl",
    "reactstrap",
    "redux",
    "redux-saga",
    "travis"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bootstrap": "4.1.1",
    "font-awesome": "4.7.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-fontawesome": "1.6.1",
    "react-intl": "2.4.0",
    "react-intl-redux": "2.0.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "reactstrap": "6.1.0",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "redux-thunk": "2.2.0",
    "roboto-fontface": "0.9.0"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.10",
    "@types/enzyme-adapter-react-16": "1.0.2",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",

    "@types/faker": "4.1.2",
    "@types/jest": "23.1.0",
    "@types/node": "10.3.3",
    "@types/react": "16.4.0",
    "@types/react-dom": "16.0.6",
    "@types/react-fontawesome": "1.6.3",
    "@types/react-intl": "2.3.8",
    "@types/react-intl-redux": "0.1.9",
    "@types/react-redux": "6.0.2",
    "@types/react-router-dom": "4.2.7",
    "@types/react-router-redux": "5.0.15",
    "@types/reactstrap": "6.0.0",
    "@types/redux-logger": "3.0.6",
    "awesome-typescript-loader": "5.1.1",
    "babel-core": "6.26.3",
    "babel-jest": "23.0.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "expose-loader": "0.7.5",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "faker": "4.1.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "history": "4.7.2",
    "jest": "23.1.0",
    "json-server": "0.14.0",
    "node-sass": "4.9.0",
    "nodemon": "1.17.5",
    "resolve-url-loader": "2.3.0",
    "sass-loader": "7.0.3",
    "sass-resources-loader": "1.3.3",
    "source-map-loader": "0.2.3",
    "style-loader": "0.21.0",
    "stylelint": "9.3.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-webpack-plugin": "0.10.5",
    "sw-precache-webpack-plugin": "0.11.5",
    "ts-jest": "22.4.6",
    "tslint": "5.10.0",
    "tslint-loader": "3.6.0",
    "typescript": "2.9.2",
    "typings-for-css-modules-loader": "1.7.0",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.6",
    "webpack-dev-server": "3.1.4",
    "webpack-manifest-plugin": "2.0.3",
    "webpack-merge": "4.1.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json",
        "useBabelrc": true
      }
    },
    "setupTestFrameworkScriptFile": "./src/tests-setup.tsx",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^.+\\.(less|scss)$": "babel-jest"
    }
  }
}
